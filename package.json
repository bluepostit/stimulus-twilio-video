{
  "name": "stimulus-twilio-video",
  "version": "1.0.2",
  "description": "Stimulus controller for video chat with Twilio",
  "type": "module",
  "source": "src/twilio-video-stimulus-controller.js",
  "exports": {
    "require": "./dist/twilio-video-stimulus-controller.cjs",
    "default": "./dist/twilio-video-stimulus-controller.modern.js"
  },
  "files": [
    "dist/*"
  ],
  "main": "./dist/twilio-video-stimulus-controller.cjs",
  "module": "./dist/twilio-video-stimulus-controller.modern.js",
  "unpkg": "./dist/twilio-video-stimulus-controller.umd.js",
  "scripts": {
    "build": "microbundle",
    "clean": "rm -f dist/*",
    "dev": "npm run clean && microbundle watch",
    "lint": "prettier --check src/*.js",
    "format": "prettier --write src/*.js",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "twilio",
    "video",
    "chat",
    "stimulus",
    "controller"
  ],
  "author": "bluepostit",
  "license": "ISC",
  "dependencies": {
    "twilio-video": "^2.18.1",
    "stimulus": "^3.0.1"
  },
  "devDependencies": {
    "microbundle": "^0.14.2",
    "prettier": "^2.5.0"
  }
}
